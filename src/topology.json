{
    "LayoutApp": {
        "appName": "LayoutApp",
        "accessUri": "http://localhost:3001/LayoutApp",
        "type": "extendable",
        "uses": ["HeaderApp"]
    },
    "staticApp": {
        "appName": "staticApp",
        "accessUri": "http://localhost:3001/staticApp",
        "route": "/static",
        "type": "page"
    },
    "HeaderApp": {
        "appName": "HeaderApp",
        "accessUri": "http://localhost:3001/HeaderApp",
        "type": "fragment"
    },
    "ContentContainer": {
        "appName": "ContentContainer",
        "accessUri": "http://localhost:3001/ContentContainer",
        "route": "/",
        "type": "page",
        "extends": "LayoutApp"
    }
}
